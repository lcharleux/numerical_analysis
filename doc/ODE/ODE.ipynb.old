    "tmax = .2\n",
    "t  = np.linspace(0., tmax, 1000) \n",
    "x0, y0   = 0., 0. \n",
    "vx0, vy0 = 1., 1.\n",
    "g = 10.\n",
    "x = vx0 * t\n",
    "y = -g  * t**2/2. + vy0 * t\n",
    "plt.plot(x, y, label = \"Exact solution\")\n",
    "plt.grid()\n",
    "plt.xlabel(\"x\")\n",
